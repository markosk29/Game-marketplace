<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <NoWin32Manifest>true</NoWin32Manifest>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\default_gamepic.png" />
    <None Remove="images\default_profilepic.png" />
    <None Remove="images\edit.png" />
    <None Remove="images\success.png" />
    <None Remove="images\warning.png" />
    <None Remove="main3.png" />
    <None Remove="main4.png" />
    <None Remove="main_img.png" />
    <None Remove="Panels\desgin.png" />
    <None Remove="Panels\recover1.png" />
    <None Remove="Panels\register1.png" />
    <None Remove="Panels\register_panel1.png" />
    <None Remove="Panels\security1.png" />
    <None Remove="toma formular.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Npgsql" Version="5.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="images\default_gamepic.png" />
    <Resource Include="images\default_profilepic.png" />
    <Resource Include="images\edit.png" />
    <Resource Include="images\success.png" />
    <Resource Include="images\warning.png" />
    <Resource Include="main3.png" />
    <Resource Include="main4.png" />
    <Resource Include="main_img.png" />
    <Resource Include="Panels\desgin.png" />
    <Resource Include="Panels\recover1.png" />
    <Resource Include="Panels\register1.png" />
    <Resource Include="Panels\register_panel1.png" />
    <Resource Include="Panels\security1.png" />
    <Resource Include="toma formular.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Panels\fonts\NanoTech Regular.otf">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Resource Update="fonts\NanoTech Regular.otf">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
